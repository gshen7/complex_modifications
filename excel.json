{
    "title": "Function keys in excel",
    "rules": [
        {
            "description": "fn 2,4 using command + number",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "2",
                        "modifiers": {
                            "mandatory": [
                                "left_gui"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "f2"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "com.microsoft.Excel"
                            ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "4",
                        "modifiers": {
                            "mandatory": [
                                "left_gui"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "f4"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_if",
                            "bundle_identifiers": [
                                "com.microsoft.Excel"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}