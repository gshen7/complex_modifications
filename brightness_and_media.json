{
    "title": "Brightness and Media control for logi",
    "rules": [
        {
            "description": "brightness",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f2",
                        "modifiers": {
                            "mandatory": [
                                "command"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "apple_top_case_display_brightness_decrement"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f3",
                        "modifiers": {
                            "mandatory": [
                                "command"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "vk_none"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f4",
                        "modifiers": {
                            "mandatory": [
                                "command"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "apple_top_case_display_brightness_increment"
                        }
                    ]
                }
            ]
        },
        {
            "description": "media",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f2",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "consumer_key_code": "scan_previous_track"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f3",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "play_or_pause"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f4",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "consumer_key_code": "scan_next_track"
                        }
                    ]
                }
            ]
        }
    ]
}